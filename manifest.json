{
    "manifest_version": 3,
    "name": "Custom New Tab Dashboard",
    "version": "1.0",
    "description": "Replace the new tab page with a custom dashboard.",
    "permissions": ["storage"],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}